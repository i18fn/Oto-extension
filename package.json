{
    "name": "oto",
    "publisher": "fn",
    "displayName": "Oto",
    "description": "Oto language for Visual Studio Code",
    "version": "0.0.3",
    "repository": {
        "url": "https://github.com/i18fn/Oto-extension"
    },
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "oto",
                "aliases": ["oto", "oto"],
                "extensions": [".oto"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "oto",
                "scopeName": "source.oto",
                "path": "./syntaxes/oto.tmLanguage.json"
            }
        ]
    }
}